{
  "name": "datable-render",
  "version": "1.0.0",
  "description": "a web data table render",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/datable-render.umd.cjs",
  "module": "dist/datable-render.js",
  "exports": {
    ".": {
      "import": "./dist/datable-render.js",
      "require": "./dist/datable-render.umd.cjs"
    }
  },
  "scripts": {
    "dev": "./node_modules/vite/bin/vite.js",
    "build": "./node_modules/vite/bin/vite.js build",
    "preview": "./node_modules/vite/bin/vite.js preview",
    "format": "./node_modules/.bin/prettier --write src test",
    "test": "./node_modules/.bin/vitest test/*.test.ts"
  },
  "author": "myliang",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.17"
  }
}
